                    </li>
                </ul>
            </div>
        </nav>



    </div>
    <div class="container mx-auto">
        <div class="justify-content-center ">

            <% 
                rowCount = 0
                videos.forEach(function(video) { %>
                <div class="col-md-6" >
                    <a href="/videos/<%= video.name %>">
                        <strong>
                            <%= video.former_name %>
                        </strong>
                        <br/>
                        <img src="../static/video/<%= video.name %>.png" />
                    </a>
                </div>
                <% rowCount++ %>
                        <% if (rowCount % 2 == 1 ){  %> </div>  <div class="justify-content-center"> <% } %>
       
           
                <%   }); %>
        </div>




