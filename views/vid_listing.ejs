<html lang="en">

   <%- include header %>
   <!-- Main component for a primary marketing message or call to action -->
      <div class="jumbotron">
        <h1>Welcome</h1>
        <p>Video Listing</p>
        <p>
          <ul>
              <% videos.forEach(function(video) { %>
                 <a href="/videos/<%= video.name %>"> <li><%= video.name %> - <%= video.userid %></li> </a>
              <% }); %>
          </ul>
        </p>
      </div>
    <%- include footer %>